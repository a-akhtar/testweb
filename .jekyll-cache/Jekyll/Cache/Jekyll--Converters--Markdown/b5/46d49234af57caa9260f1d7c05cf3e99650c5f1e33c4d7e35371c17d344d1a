  <span class="na">bibtex_show</span> <span class="p">=</span> <span class="s">{true}</span><span class="p">,</span>
  <span class="na">author</span> <span class="p">=</span> <span class="s">{Tribou, M. and Akhtar, A. and Waslander, S. L.}</span><span class="p">,</span>
  <span class="na">booktitle</span> <span class="p">=</span> <span class="s">{American Institute of Aeronautics and Astronautics (AIAA) Guidance, Navigation, and Control Conference}</span><span class="p">,</span>
  <span class="na">title</span> <span class="p">=</span> <span class="s">{Relative Position-Based Visual Servoing Control for Quadrotors}</span><span class="p">,</span>
  <span class="na">year</span> <span class="p">=</span> <span class="s">{2012}</span><span class="p">,</span>
